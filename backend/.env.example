# FitChallenge Backend 环境配置示例
# 复制此文件为 .env 并填入实际值

# 服务器配置
NODE_ENV=development
PORT=3002

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=fitchallenge
DB_USER=root
DB_PASSWORD=fitchallenge123

# JWT 安全配置
JWT_SECRET=fitchallenge_secret_key
JWT_EXPIRES_IN=24h

# CORS 配置
CORS_ORIGIN=http://localhost:8080,http://localhost:8081

# 速率限制配置
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# 日志配置
LOG_LEVEL=info
LOG_FILE=app.log